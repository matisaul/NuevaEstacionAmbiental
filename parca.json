         "script":"var correrPromedio = false;\n var desfase = 15;\n if(correrPromedio) desfase = desfase *2;\n var valorCR={};\n    valorCR.WS          = WS.value;\n    valorCR.WD          = WD.value;\n    valorCR.MP25        = MP25.value;\n    valorCR.MP10        = MP10.value;\n    valorCR.MP1         = MP1.value;\n    valorCR.MPTOTALES   = MPTOTALES.value;\n    valorCR.TA          = TA.value;\n    valorCR.RH          = RH.value;\n    valorCR.SIGMA       = SIGMA.value; \n    valorCR.NIEVE       = NIEVE.value;\n    valorCR.LLUVIA      = LLUVIA.value;\n    valorCR.MP10N       = MP10N.value;\n    valorCR.MP25N       = MP25N.value;\n    valorCR.UV          = UV.value;\n    valorCR.RSOLAR      = RSOLAR.value;\n    valorCR.TI          = TI.value;\n    valorCR.PRESION     = PRESION.value; \n\t\n\t\nvar tiempoCR={};\n    tiempoCR.WS          = WS.time;\n    tiempoCR.WD          = WD.time;\n    tiempoCR.MP25        = MP25.time;\n    tiempoCR.MP10        = MP10.time;\n    tiempoCR.MP1         = MP1.time;\n    tiempoCR.MPTOTALES   = MPTOTALES.time;\n    tiempoCR.TA          = TA.time;\n    tiempoCR.RH          = RH.time;\n    tiempoCR.SIGMA       = SIGMA.time; \n    tiempoCR.NIEVE       = NIEVE.time;\n    tiempoCR.LLUVIA      = LLUVIA.time;\n    tiempoCR.MP10N       = MP10N.time;\n    tiempoCR.MP25N       = MP25N.time;\n    tiempoCR.UV          = UV.time;\n    tiempoCR.RSOLAR      = RSOLAR.time;\n    tiempoCR.TI          = TI.time;\n    tiempoCR.PRESION     = PRESION.time; \n\t\n\t\nvar valor15={};\n    valor15.WS          = WS_15.value;\n    valor15.WD          = WD_15.value;\n    valor15.MP25        = MP25_15.value;\n    valor15.MP10        = MP10_15.value;\n    valor15.MP1         = MP1_15.value;\n    valor15.MPTOTALES   = MPTOTALES_15.value;\n    valor15.TA          = TA_15.value;\n    valor15.RH          = RH_15.value;\n    valor15.SIGMA       = SIGMA_15.value; \n    valor15.NIEVE       = NIEVE_15.value;\n    valor15.LLUVIA      = LLUVIA_15.value;\n    valor15.MP10N       = MP10N_15.value;\n    valor15.MP25N       = MP25N_15.value;\n    valor15.UV          = UV_15.value;\n    valor15.RSOLAR      = RSOLAR_15.value;\n    valor15.TI          = TI_15.value;\n    valor15.PRESION     = PRESION_15.value; \n\t\n\nvar tiempo15={};\n    tiempo15.WS          = WS_15.time;\n    tiempo15.WD          = WD_15.time;\n    tiempo15.MP25        = MP25_15.time;\n    tiempo15.MP10        = MP10_15.time;\n    tiempo15.MP1         = MP1_15.time;\n    tiempo15.MPTOTALES   = MPTOTALES_15.time;\n    tiempo15.TA          = TA_15.time;\n    tiempo15.RH          = RH_15.time;\n    tiempo15.SIGMA       = SIGMA_15.time; \n    tiempo15.NIEVE       = NIEVE_15.time;\n    tiempo15.LLUVIA      = LLUVIA_15.time;\n    tiempo15.MP10N       = MP10N_15.time;\n    tiempo15.MP25N       = MP25N_15.time;\n    tiempo15.UV          = UV_15.time;\n    tiempo15.RSOLAR      = RSOLAR_15.time;\n    tiempo15.TI          = TI_15.time;\n    tiempo15.PRESION     = PRESION_15.time; \n\n\nfor (var concentracion in valor15){\n\tif(tiempo15[concentracion] == -1 && valor15[concentracion] === 0){\n\t    \/\/print(\"No hay data\");\n\t\tif(concentracion =='WS') {\n            WS_15.set(9999,formatearFecha(WS.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='WD') {\n            WD_15.set(9999,formatearFecha(WD.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='MP25') {\n            MP25_15.set(9999,formatearFecha(MP25.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='MP10') {\n            MP10_15.set(9999,formatearFecha(MP10.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='MP1') {            \n            MP1_15.set(9999,formatearFecha(MP1.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='MPTOTALES') {            \n            MPTOTALES_15.set(9999,formatearFecha(MPTOTALES.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='RH') {            \n            RH_15.set(9999,formatearFecha(RH.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='TA') {            \n            TA_15.set(9999,formatearFecha(TA.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='SIGMA') {            \n            SIGMA_15.set(9999,formatearFecha(SIGMA.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='NIEVE') {\n            NIEVE_15.set(9999,formatearFecha(NIEVE.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='LLUVIA') {\n            LLUVIA_15.set(9999,formatearFecha(LLUVIA.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='MP10N') {\n            MP10N_15.set(9999,formatearFecha(MP10N.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='MP25N') {            \n            MP25N_15.set(9999,formatearFecha(MP25N.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='UV') {            \n            UV_15.set(9999,formatearFecha(UV.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='RSOLAR') {            \n            RSOLAR_15.set(9999,formatearFecha(RSOLAR.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='PRESION') {            \n            RH_15.set(9999,formatearFecha(RH.past(YEAR,3).firstTime));\n        }\n        else if(concentracion =='TI') {            \n            TI_15.set(9999,formatearFecha(TI.past(YEAR,3).firstTime));\n        }\n\n\t\t\n\t}\n\telse{\n\t\tvar ultimoPromedioScript   = parseInt(tiempo15[concentracion]);\n\t\tvar ultimoPromedioCR1000           = parseInt(tiempoCR[concentracion]);\n\t\tvar diferenciaBloque = Math.abs(parseInt(((ultimoPromedioCR1000 - ultimoPromedioScript)\/1000)\/60));\n\t\tvar vueltas                 = parseInt((diferenciaBloque)\/30);\n\n\n\t\t\/\/print(\"ultimo Promedio dia: \" + new Date(ultimoPromedioScript));\n\t\t\/\/print(\"ultimo prom hora: \" + new Date(ultimoPromedioCR1000));\n\n\t\t\/\/print(\"vueltas: \" + vueltas);\n\t\t\/\/print(\"diferencia Bloque: \" + diferenciaBloque);\n\n\t\tvar principio;\n\t\tvar fin;\n\n\t\tif (vueltas > 30){\n\t\t\tvueltas = 30;\n\t\t}\n\n\t\tif(vueltas >= 1){\n\t\t\tfor(y=0; y < vueltas; y++){\n\n\t\t\t\tvar fechaUltimoPromedio = new Date(ultimoPromedioScript);\n\t\t\t\t\n\n\t\t\t\tprincipio = fechaUltimoPromedio.setMinutes(fechaUltimoPromedio.getMinutes() + (desfase -1) ); \n\t\t\t\tfin = fechaUltimoPromedio.setMinutes(fechaUltimoPromedio.getMinutes() + 2);\n\t\t\t\t\n\t\t\t\tultimoPromedioScript = new Date(ultimoPromedioScript);\n\t\t\t\tultimoPromedioScript = ultimoPromedioScript.setMinutes(ultimoPromedioScript.getMinutes() + 15);\n\t\t   \n\t\t\t\t\/\/print(\"desde: \" + new Date(principio));\n\t\t\t\t\/\/print(\"hasta: \" + new Date(fin));\n\n\t\t\t\tvar DatosPromedio;\n                    \n                if(concentracion =='WS') {\n                    DatosPromedio = WS.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='WD') {\n                    DatosPromedio = WD.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='MP25') {\n                    DatosPromedio = MP25.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='MP10') {\n                    DatosPromedio = MP10.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='MP1') {            \n                    DatosPromedio = MP1.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='MPTOTALES') {            \n                    DatosPromedio = MPTOTALES.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='RH') {            \n                    DatosPromedio = RH.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='TA') {            \n                    DatosPromedio = TA.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='SIGMA') {            \n                    DatosPromedio = SIGMA.pointValuesBetween(principio,fin);\n                }   \t\n                else if(concentracion =='NIEVE') {\n                    DatosPromedio = NIEVE.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='LLUVIA') {\n                    DatosPromedio = LLUVIA.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='MP10N') {\n                    DatosPromedio = MP10N.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='MP25N') {            \n                    DatosPromedio = MP25N.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='UV') {            \n                    DatosPromedio = UV.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='RSOLAR') {            \n                    DatosPromedio = RSOLAR.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='TI') {            \n                    DatosPromedio = TI.pointValuesBetween(principio,fin);\n                }\n                else if(concentracion =='PRESION') {            \n                    DatosPromedio = PRESION.pointValuesBetween(principio,fin);\n                }   \t\n\t\t\t\t\t\t\t\t\t\n\t\t\t\t\/\/print(\"datospromedio: \" + DatosPromedio.get(0).value);  \n\t\t\t\t\/\/print(\"UltimoPaso \" + new Date(ultimoPromedioScript));\n        \t    var largo = parseFloat(DatosPromedio.size());\n\t\t\t\t\/\/print(\"LARGO: \" + largo);\n\t\t\t\n\t\t\t\tif (largo == 1){\n\t\t\t\t\tvar promedio = parseFloat(DatosPromedio.get(0).value);\n\t\t\t\t}\n\t\t\t\telse {\n\t\t\t\t\tvar promedio = 0;\n\t\t\t\t}\n\t\t\t\t\t\n\t\t\t\t\t\n                if(concentracion =='WS') {\n                    \/\/print(\"concentracion \" + concentracion);\n                    WS_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='WD') {\n                    WD_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='MP25') {\n                    MP25_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='MP10') {\n                    MP10_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='MP1') {            \n                    MP1_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='MPTOTALES') {            \n                    MPTOTALES_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='RH') {            \n                    RH_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='TA') {            \n                    TA_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='SIGMA') {            \n                    SIGMA_15.set(promedio,ultimoPromedioScript);\n                }   \t\n                else if(concentracion =='NIEVE') {\n                    NIEVE_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='LLUVIA') {\n                    LLUVIA_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='MP10N') {\n                    MP10N_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='MP25N') {            \n                    MP25N_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='UV') {            \n                    UV_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='RSOLAR') {            \n                    RSOLAR_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='TI') {            \n                    TI_15.set(promedio,ultimoPromedioScript);\n                }\n                else if(concentracion =='PRESION') {            \n                    PRESION_15.set(promedio,ultimoPromedioScript);\n                }  \n                \n\t\t\t}\n\t\t}\n\t}\n}\n\nfunction formatearFecha(time){\n    var tiempo = parseInt(time);\n    var tiempoDefault    = new Date(tiempo);\n    tiempoDefault.setMinutes(tiempoDefault.getMinutes() - desfase);\n    return tiempoDefault;\n}",
